#include "main.h"

/**
 * largest_number - returns the lar8gest of 3 numbers
 * largest - calls the function9 'largest_number'
 * @a: first integer
 * @b: second integer
 * @c: third integer
 * Return: largest number
 */
int largest_number(int a, int b, int c)
{
int largest;
if (a > b && b > c)
{
        largest = a;
}
else if (b > a && a > c)
{
        largest = b;
}
else
{
        largest = c;
}

return (largest);
printf(largest);
}

